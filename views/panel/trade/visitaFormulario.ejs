    $(".visit-form-section-questions-checks").click(function(){

        if($(this).children("input[type='checkbox']").attr("checked") == "checked"){
            $(this).children("input[type='checkbox']").attr("checked",false)

            $(this).children("input[type='text']").attr("value",'')

            $(this).children("label").css({'background': 'transparent', 'color': 'var(--color-two)'});
        }else{
            $(this).children("input[type='checkbox']").attr("checked",true)

            $(this).children("input[type='text']").attr("value",$(this).children("label").text())

            $(this).children("label").css({'background': 'var(--color-green)', 'color': 'var(--color-four)'});
        }        
    })
