<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.1/moment.min.js"></script>
<script src="https://cdn.socket.io/4.4.1/socket.io.min.js"></script>
<script>
    //Iniciando socket
    //const socket = io("http://localhost:8080")

    async function showUsersShops (cnpjShop, nameShop){
        await socket.emit("getInfoUserTrade", {cnpjShop: cnpjShop, nameShop})

        let dataRequest;
        let validate = await socket.on("getInfoUserTradeAll", (data) => {
            return data != '' ? dataRequest = data : dataRequest = null
        })

        setTimeout(() => {
            $.fn.loadSpin(0)//Tela de carregando
            $("#visit-form-users-list").fadeIn().css({'display': 'flex'})
            $("#visit-form-users-box").empty()

            if(dataRequest != null){
                //Exibindo os usuarios
                $("#title-users-shops").append("<span>Usuarios da loja <b>"+nameShop+"</b><span>")
                dataRequest.forEach(element => {
                    //Valida se o usuario é representante
                    if(element.jcv_userCassification == 4){
                        $("#visit-form-users-box").append
                        (
                        `<div class="visit-form-users-list-box">
                                <span><b>${element.jcv_userNamePrimary}</b></span>
                        </div>`
                        )
                    }else{
                        $("#visit-form-users-box").append
                        (
                        `<div class="visit-form-users-list-box">
                                <span>${element.jcv_userNamePrimary}</span>
                        </div>`
                        )
                    }
                });
            }else{
                $("#visit-form-users-box").append
                (
                `<span style="font-size: 0.8rem">Nenhum usuario encontrado para a loja <b>${nameShop}</b>.</span>`
                )
            }
        }, 500);
    }


    $("#date-form-now").val(moment().format("YYYY-MM-DD"))


    //Array com o CNPJ puro e NOME SOCIAL puro
    const allInofShops = '<%- arrayShops %>'
    const convertShops = allInofShops.split('|*|')
    
    //Pegando so os CNPJ's
    const allCNPJ = []
    convertShops.forEach(element => {
        allCNPJ.push(element.split(' @# ')[0])
    })

    //Pegando so os NOME SOCIAL da loja
    const allShopsName = []
    convertShops.forEach(element => {
        allShopsName.push(element.split(' @# ')[1])
    })
    allShopsName.pop()

    //Autocomplete do CNPJ
    let valueSetCNPJ = '';
    $("#input-shop-cnpj").autocomplete({
        source: function(request, response) {
            var results = $.ui.autocomplete.filter(allCNPJ, request.term);        
            response(results.slice(0, 10));
            
            $('.ui-menu-item-wrapper').click(function () {

                convertShops.forEach(element => {

                //Aqui eu verifico se o NOME existe no element, caso sim ele pega busca novamente
                //utilizando o nome e o cnpj na matriz pricipal
                //1° Busco a string, 2° busco o valor na matriz
                let searchInput = element.indexOf($(this).text());

                //Achamos algo na string
                if(searchInput > -1 ){
                    valueSetShop = element.split(' @# ')[1]
                }

                });

                $.fn.loadSpin(1)//Tela de carregando

                $("#input-shop-name").val(valueSetShop)
                showUsersShops ($(this).text(), valueSetShop)//Pegando os usuarios das lojas
                valueSetShop = undefined;//Após a inserção, ele reseta o input
            })
        }
    })

    //Autocomplete do NOME SOCIAL
    $("#input-shop-name").autocomplete({
        source: function(request, response) {
            var results = $.ui.autocomplete.filter(allShopsName, request.term);        
            response(results.slice(0, 10));
            
            let valueSetShop = 0;
            $('.ui-menu-item-wrapper').click(function () {
                $.fn.loadSpin(1)//Carregando..

                convertShops.forEach(element => {

                    //Aqui eu verifico se o NOME existe no element, caso sim ele pega busca novamente
                    //utilizando o nome e o cnpj na matriz pricipal
                    //1° Busco a string, 2° busco o valor na matriz
                    let searchInput = element.indexOf($(this).text());

                    //Achamos algo na string
                    if(searchInput > -1 ){
                        valueSetShop = element.split(' @# ')[0]
                    }

                });

                $("#input-shop-cnpj").val(valueSetShop)
                showUsersShops (valueSetShop, $(this).text())//Pegando os usuarios das lojas
                valueSetShop = undefined
            })
        }
    })


    
    
