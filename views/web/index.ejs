<script>
    //Script valid pwa
    let displayApp = $("#system-display-app")//Display

    //PWA
    let deferredPrompt; // Allows to show the install prompt

    window.addEventListener('beforeinstallprompt', (e) => {
        //console.log('sem app')
        appInstalled = false;

        e.preventDefault()
        deferredPrompt = e;

        if(Cookies.get('NOT-INSTALL-APP-BOX') == undefined){
            displayApp.fadeIn().css('display','flex')
            $("#sys-install-app-box").fadeIn().css('display','flex')
        }
    });

    function installApp() {
        // Show the prompt
        deferredPrompt.prompt();
        //setupButton.disabled = true;
        // Wait for the user to respond to the prompt
        deferredPrompt.userChoice
            .then((choiceResult) => {
                if (choiceResult.outcome === 'accepted') {
                    console.log('PWA setup accepted');
                    // hide our user interface that shows our A2HS button
                    displayApp.fadeOut()

                    setTimeout(() => {
                        alert('Seu aplicativo foi instalado! Procure por "JCV" em sua lista de apps')
                   }, 1000);
                } else {
                    console.log('PWA setup rejected');
                }
                deferredPrompt = null;
            })
    }

    $("#close-display-app").click(function(){
        $("#sys-install-app-box").fadeOut()

        setTimeout(() => {
            $("#sys-install-app-alert").fadeIn().css('display','flex')

            setTimeout(() => {
                Cookies.set('NOT-INSTALL-APP-BOX', true, {expires: 7});
                displayApp.fadeOut()
            }, 3000);
        }, 500);
    })
    
</script>
